{
  "root": true,
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "airbnb-base/legacy"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-console": ["off", { "allow": ["log"] }],
    "comma-dangle": ["error", "always-multiline"],
    "linebreak-style": 0,
    "global-require": 0
  }
}